                android:textSize="@dimen/setting_text_size"

                android:layout_toRightOf="@+id/number"
                android:layout_marginTop="@dimen/default_margin_size"
                />
<!--                android:layout_below="@id/setting_recalibrate"-->
            <TextView
                android:id="@+id/target_point_Y"
                android:layout_width="@dimen/setting_margin_width"
                android:layout_height="wrap_content"
                android:text="@string/target_point_Y"
                android:textColor="@color/black_color"
                android:textSize="@dimen/setting_text_size"

                android:layout_toRightOf="@+id/target_point_X"
                android:layout_marginTop="@dimen/default_margin_size"
                />
<!--                android:layout_below="@id/setting_recalibrate"-->
            <TextView
                android:id="@+id/collect_point_X"
                android:layout_width="@dimen/setting_margin_width"
                android:layout_height="wrap_content"
                android:text="@string/collect_point_X"
                android:textColor="@color/black_color"
                android:textSize="@dimen/setting_text_size"

                android:layout_toRightOf="@+id/target_point_Y"
                android:layout_marginTop="@dimen/default_margin_size"
                />
<!--                android:layout_below="@id/setting_recalibrate"-->
            <TextView
                android:id="@+id/collect_point_Y"
                android:layout_width="@dimen/setting_margin_width"
                android:layout_height="wrap_content"
                android:text="@string/collect_point_Y"
