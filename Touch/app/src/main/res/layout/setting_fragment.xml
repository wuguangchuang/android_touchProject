                android:layout_height="@dimen/button_height"
                android:textSize="@dimen/setting_text_size"
                android:background="@color/normalColor"
                android:textColor="@color/black_color"
                android:text="@string/save"

                android:layout_toRightOf="@id/setting_setting"
                android:layout_marginTop="@dimen/default_margin_size"
                android:layout_marginRight="@dimen/default_margin_size"
                />
            <Button
                android:id="@+id/setting_read"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/button_height"
                android:textSize="@dimen/setting_text_size"
                android:background="@color/normalColor"
                android:textColor="@color/black_color"
                android:text="@string/read"

                android:layout_toRightOf="@id/setting_save"
                android:layout_marginTop="@dimen/default_margin_size"
                android:layout_marginRight="@dimen/default_margin_size"
                />
            <Button
                android:id="@+id/setting_hid_data"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/button_height"
                android:textSize="@dimen/setting_text_size"
                android:background="@color/normalColor"
                android:textColor="@color/black_color"
                android:text="@string/hid_data"

                android:layout_toRightOf="@id/setting_read"
                android:layout_marginTop="@dimen/default_margin_size"
                android:layout_marginRight="@dimen/default_margin_size"
                />
            <Button
                android:id="@+id/setting_recalibrate_restore_factory"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/button_height"
                android:textSize="@dimen/setting_text_size"
                android:background="@color/normalColor"
                android:textColor="@color/black_color"
                android:text="@string/restore_factory"

                android:layout_toRightOf="@id/setting_hid_data"
                android:layout_marginTop="@dimen/default_margin_size"
                android:layout_marginRight="@dimen/default_margin_size"
                />
            </RelativeLayout>
            </HorizontalScrollView>
<!--   ########  坐标TextView ###################-->
            <RelativeLayout
                android:id="@+id/setting_hid_data_point"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/setting_btn_scroll">

