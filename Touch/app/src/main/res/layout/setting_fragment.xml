                android:layout_marginLeft="@dimen/default_margin_size"

                android:layout_marginTop="@dimen/default_margin_size"
                />
            <Button
                android:id="@+id/setting_recalibrate"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/button_height"
                android:textSize="@dimen/setting_text_size"
                android:background="@color/normalColor"
                android:textColor="@color/black_color"
                android:text="@string/calibrate"

                android:layout_toRightOf="@id/calibration"
                android:layout_marginTop="@dimen/default_margin_size"
                android:layout_marginRight="@dimen/default_margin_size"
                />
            <Button
                android:id="@+id/setting_refresh"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/button_height"
                android:textSize="@dimen/setting_text_size"
                android:background="@color/normalColor"
                android:textColor="@color/black_color"
                android:text="@string/refresh"

                android:layout_toRightOf="@id/setting_recalibrate"
                android:layout_marginTop="@dimen/default_margin_size"
                android:layout_marginRight="@dimen/default_margin_size"
                />
            <Button
                android:id="@+id/setting_setting"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/button_height"
                android:textSize="@dimen/setting_text_size"
                android:background="@color/normalColor"
                android:textColor="@color/black_color"
                android:text="@string/setting"

                android:layout_toRightOf="@id/setting_refresh"
