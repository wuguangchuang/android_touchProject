                android:layout_marginRight="@dimen/default_margin_size"
                android:layout_toRightOf="@id/setting_touch_rollover_text"
                android:layout_below="@+id/line1"
                android:button="@drawable/checked_rect_selected"
                android:textSize="@dimen/setting_text_size"
                android:layout_marginTop="@dimen/default_margin_size"
                />

            <CheckBox
                android:id="@+id/setting_y_rollover"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/y_rollover"
