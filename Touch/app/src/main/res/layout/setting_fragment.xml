                android:layout_height="@dimen/line_height"
                android:background="@drawable/black_board_rect"
                android:layout_below="@id/coordID"
                android:layout_marginTop="@dimen/default_margin_size"
                android:layout_marginBottom="@dimen/default_margin_size"
                android:layout_marginLeft="@dimen/default_margin_size"/>

            <!--            ##############触摸框翻转############-->
            <TextView
                android:id="@+id/setting_touch_rollover_text"
                android:layout_width="@dimen/setting_textView_size"
                android:layout_height="wrap_content"
                android:text="@string/touch_rollover"
                android:textColor="@color/black_color"
                android:textSize="@dimen/setting_text_size"
                android:layout_marginLeft="@dimen/default_margin_size"
                android:layout_below="@id/line1"
                android:layout_marginTop="@dimen/default_margin_size"
