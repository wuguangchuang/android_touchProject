                android:layout_width="@dimen/setting_textView_size"
                android:layout_height="wrap_content"
                android:text="@string/turn_screen_clockwise"
                android:textColor="@color/black_color"
                android:textSize="@dimen/setting_text_size"
                android:layout_marginLeft="@dimen/default_margin_size"
                android:layout_below="@id/setting_touch_group"
                android:layout_marginTop="@dimen/default_margin_size"
                />
            <RadioGroup
                android:id="@+id/setting_turn_screen_clockwise_group"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_toRightOf="@+id/setting_turn_screen_clockwise"
                android:layout_below="@id/setting_touch_group"
                >

                <RadioButton
                    android:id="@+id/setting_screen_0"
                    android:layout_width="@dimen/setting_checkBox_size"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/setting_text_size"
                    android:checked="true"
                    android:layout_marginTop="@dimen/default_margin_size"
                    android:button="@drawable/checked_circle_selected"
                    />
                <RadioButton
                    android:id="@+id/setting_screen_90"
                    android:layout_width="@dimen/setting_checkBox_size"
                    android:layout_height="wrap_content"
                    android:text="90"
                    android:textColor="@color/black_color"
                    android:layout_toRightOf="@id/setting_screen_0"
                    android:textSize="@dimen/setting_text_size"
                    android:layout_marginTop="@dimen/default_margin_size"
                    android:button="@drawable/checked_circle_selected"
                    />
                <RadioButton
                    android:id="@+id/setting_screen_180"
                    android:layout_width="@dimen/setting_checkBox_size"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/setting_screen_90"
                    android:text="180"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/setting_text_size"
                    android:layout_marginTop="@dimen/default_margin_size"
                    android:button="@drawable/checked_circle_selected"
                    />
                <RadioButton
                    android:id="@+id/setting_screen_270"
                    android:layout_width="@dimen/setting_checkBox_size"
                    android:layout_height="wrap_content"
                    android:text="270"
                    android:textColor="@color/black_color"
                    android:layout_toRightOf="@id/setting_screen_180"
                    android:textSize="@dimen/setting_text_size"
                    android:layout_marginTop="@dimen/default_margin_size"
                    android:button="@drawable/checked_circle_selected"
                    />

            </RadioGroup>
            <Button
                android:id="@+id/setting_restore_factory_screen"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/button_height"
                android:textSize="@dimen/setting_text_size"
                android:background="@color/normalColor"
                android:textColor="@color/black_color"
                android:text="@string/restore_factory"
                android:layout_below="@id/setting_touch_group"
                android:layout_toRightOf="@id/setting_turn_screen_clockwise_group"
                android:layout_marginTop="@dimen/default_margin_size"
                />

            <!--  ############## MAC OS ############-->
            <TextView
                android:id="@+id/setting_MAC_OS"
                android:layout_width="@dimen/setting_textView_size"
                android:layout_height="wrap_content"
                android:text="@string/MAC_OS"
                android:textColor="@color/black_color"
                android:textSize="@dimen/setting_text_size"
                android:layout_marginLeft="@dimen/default_margin_size"
                android:layout_below="@id/setting_turn_screen_clockwise_group"
                android:layout_marginTop="@dimen/default_margin_size"
                />
            <RadioGroup
                android:id="@+id/setting_MAC_OS_group"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_toRightOf="@+id/setting_MAC_OS"
                android:layout_below="@id/setting_turn_screen_clockwise_group"
                >

                <RadioButton
                    android:id="@+id/setting_MAC_OS_10.9_following"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/MAC_OS_10.9_following"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/setting_text_size"
                    android:layout_marginTop="@dimen/default_margin_size"
                    android:layout_marginRight="@dimen/default_margin_size"
                    android:button="@drawable/checked_circle_selected"
                    />
                <RadioButton
                    android:id="@+id/setting_MAC_OS_10.9_above"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/MAC_OS_10.9_above"
                    android:textColor="@color/black_color"
                    android:layout_toRightOf="@id/setting_MAC_OS_10.9_following"
                    android:textSize="@dimen/setting_text_size"
                    android:layout_marginTop="@dimen/default_margin_size"
                    android:button="@drawable/checked_circle_selected"
                    android:layout_marginRight="@dimen/default_margin_size"
                    />

            </RadioGroup>
            <Button
                android:id="@+id/setting_restore_factory_setting_MAC_OS"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/button_height"
                android:textSize="@dimen/setting_text_size"
                android:background="@color/normalColor"
                android:textColor="@color/black_color"
                android:text="@string/restore_factory"
                android:layout_below="@id/setting_turn_screen_clockwise_group"
                android:layout_toRightOf="@id/setting_MAC_OS_group"
                android:layout_marginTop="@dimen/default_margin_size"
                />
