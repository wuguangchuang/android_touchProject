<!--        android:layout_height="match_parent">-->


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbarThumbVertical="@color/gray_color"
            android:scrollbarSize="@dimen/five">

            <RelativeLayout
                android:id="@+id/coordID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <TextView
                    android:id="@+id/setting_USB_text"
                    android:layout_width="@dimen/setting_textView_size"
                    android:layout_height="wrap_content"
                    android:text="@string/USB_model"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/setting_text_size"
                    android:layout_marginLeft="@dimen/default_margin_size"
                    android:layout_marginTop="@dimen/default_margin_size"
                    />
                <CheckBox
                    android:id="@+id/USB_check"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/default_margin_size"
                    android:layout_toRightOf="@id/setting_USB_text"
                    android:button="@drawable/checked_rect_selected"
                    android:textSize="@dimen/setting_text_size"
                    android:layout_marginTop="@dimen/default_margin_size"
                    />
                <RadioGroup
                    android:id="@+id/USB_radio_group"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_toRightOf="@+id/USB_check"
