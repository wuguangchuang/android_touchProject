                        android:textSize="@dimen/setting_text_size"
                        android:layout_marginTop="@dimen/default_margin_size"
                        android:button="@drawable/checked_circle_selected"
                        />

                </RadioGroup>
                <TextView
                    android:id="@+id/setting_UART_text"
                    android:layout_width="@dimen/setting_textView_size"
                    android:layout_height="wrap_content"
                    android:text="@string/UART_model"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/setting_text_size"
                    android:layout_marginLeft="@dimen/default_margin_size"
                    android:layout_below="@id/setting_USB_text"
                    android:layout_marginTop="@dimen/default_margin_size"
                    />
                <CheckBox
                    android:id="@+id/setting_UART_check"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/default_margin_size"
                    android:layout_toRightOf="@id/setting_UART_text"
                    android:layout_below="@+id/USB_check"
                    android:button="@drawable/checked_rect_selected"
                    android:textSize="@dimen/setting_text_size"
                    android:layout_marginTop="@dimen/default_margin_size"
                    />
