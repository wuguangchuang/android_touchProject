            <TextView
                android:id="@+id/nullTextView1"
                android:layout_width="@dimen/setting_textView_size"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/default_margin_size"
                android:layout_marginTop="@dimen/default_margin_size"
                />
<!--                android:layout_below="@id/setting_recalibrate"-->
            <TextView
                android:id="@+id/number"
                android:layout_width="@dimen/setting_margin_width"
                android:layout_height="wrap_content"
                android:text="@string/number"
                android:textColor="@color/black_color"
                android:textSize="@dimen/setting_text_size"
                android:layout_toRightOf="@+id/nullTextView1"
                android:layout_marginTop="@dimen/default_margin_size"
