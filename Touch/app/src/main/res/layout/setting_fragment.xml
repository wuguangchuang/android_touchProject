            <TextView
                android:id="@+id/line2"
                android:layout_width="1000dp"
                android:layout_height="@dimen/line_height"
                android:background="@drawable/black_board_rect"
                android:layout_below="@id/setting_MAC_OS_group"
                android:layout_marginTop="@dimen/default_margin_size"
                android:layout_marginBottom="@dimen/default_margin_size"
                android:layout_marginLeft="@dimen/default_margin_size"/>
<!--  ###############   校准    ##############                -->
            <HorizontalScrollView
                android:id="@+id/setting_btn_scroll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/line2">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


            <TextView
