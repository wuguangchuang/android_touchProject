                android:layout_width="wrap_content"
                android:layout_height="@dimen/button_height"
                android:textSize="@dimen/setting_text_size"
                android:background="@color/normalColor"
                android:textColor="@color/black_color"
                android:text="@string/restore_factory"
                android:layout_below="@id/line1"
               android:layout_toRightOf="@id/setting_y_rollover"
                android:layout_marginTop="@dimen/default_margin_size"
                />
<!--            ##############触摸框顺时针旋转############-->
            <TextView
                android:id="@+id/setting_turn_touch_clockwise"
                android:layout_width="@dimen/setting_textView_size"
                android:layout_height="wrap_content"
                android:text="@string/turn_touch_clockwise"
                android:textColor="@color/black_color"
                android:textSize="@dimen/setting_text_size"
                android:layout_marginLeft="@dimen/default_margin_size"
                android:layout_below="@id/setting_touch_rollover_text"
                android:layout_marginTop="@dimen/default_margin_size"
                />
            <RadioGroup
                android:id="@+id/setting_touch_group"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_toRightOf="@id/setting_turn_touch_clockwise"
                android:layout_below="@id/setting_touch_rollover_text"
                >

                <RadioButton
                    android:id="@+id/setting_touch_0"
                    android:layout_width="@dimen/setting_checkBox_size"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/setting_text_size"
                    android:checked="true"
                    android:layout_marginTop="@dimen/default_margin_size"
                    android:button="@drawable/checked_circle_selected"
                    />
                <RadioButton
                    android:id="@+id/setting_touch_90"
                    android:layout_width="@dimen/setting_checkBox_size"
                    android:layout_height="wrap_content"
                    android:text="90"
                    android:textColor="@color/black_color"
                    android:layout_toRightOf="@id/setting_touch_0"
                    android:textSize="@dimen/setting_text_size"
                    android:layout_marginTop="@dimen/default_margin_size"
                    android:button="@drawable/checked_circle_selected"
                    />
                <RadioButton
                    android:id="@+id/setting_touch_180"
                    android:layout_width="@dimen/setting_checkBox_size"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/setting_touch_90"
                    android:text="180"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/setting_text_size"
                    android:layout_marginTop="@dimen/default_margin_size"
                    android:button="@drawable/checked_circle_selected"
                    />
                <RadioButton
                    android:id="@+id/setting_touch_270"
                    android:layout_width="@dimen/setting_checkBox_size"
                    android:layout_height="wrap_content"
                    android:text="270"
                    android:textColor="@color/black_color"
                    android:layout_toRightOf="@id/setting_touch_180"
                    android:textSize="@dimen/setting_text_size"
                    android:layout_marginTop="@dimen/default_margin_size"
                    android:button="@drawable/checked_circle_selected"
                    />

            </RadioGroup>
            <Button
                android:id="@+id/setting_restore_factory_touch"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/button_height"
                android:textSize="@dimen/setting_text_size"
                android:background="@color/normalColor"
                android:textColor="@color/black_color"
                android:text="@string/restore_factory"
                android:layout_below="@id/setting_touch_rollover_text"
                android:layout_toRightOf="@id/setting_touch_group"
                android:layout_marginTop="@dimen/default_margin_size"
